{
  "name": "@Loram-Technologies/rail_dev_ops",
  "version": "1.0.0",
  "description": "",
  "main": "cloudformation.js",
  "scripts": {
    "test": "node test.js",
    "deploy-stack": "cdk deploy --all",
    "destroy-stack": "cdk destroy --all",
    "destroy-services": "node -r dotenv/config ./scripts/deleteRailStack.js",
    "cb-mac": "aws codebuild start-build --project-name RailDevOps-Direct --source-version $(git rev-parse --abbrev-ref HEAD)",
    "cb-win": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do aws codebuild start-build --project-name RailDevOps-Direct --source-version %i",
    "get-packages": "node -r dotenv/config ./scripts/getPackageVersions.js",
    "configure-user": "node -r dotenv/config ./scripts/createUserManagement.js",
    "configure-api": "node -r dotenv/config ./scripts/createCustomerApi.js",
    "configure-dns": "node -r dotenv/config ./core/config/configureDns.js",
    "update-pipeline": "node scripts/updateCodePipeline.js",
    "publish-services": "node -r dotenv/config ./scripts/publishPackages.js",
    "get-versions": "node -r dotenv/config ./scripts/getPackageVersions.js",
    "ca:login": "aws codeartifact login --tool npm --repository loram-digital --domain loram-digital-domain --domain-owner 569575870388 --namespace @loram"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-cdk-lib": "^2.17.0",
    "constructs": "^10.0.0",
    "aws-sdk": "^2.644.0",
    "dot-object": "^2.1.3",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "aws-cdk": "2.17.0",
    "colors": "^1.4.0",
    "minimist": "^1.2.5"
  }
}
